{
  "name": "shoppro",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "seeder": "concurrently \"data:destroy\" \"npm run data:import\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAVOINHO/shopPro.git"
  },
  "author": "Trung Kien",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CAVOINHO/shopPro/issues"
  },
  "homepage": "https://github.com/CAVOINHO/shopPro#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.25.1",
    "colors": "^1.4.0",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "firebase": "^8.3.0",
    "google-auth-library": "^7.0.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "notistack": "^1.0.3",
    "react-image-file-resizer": "^0.4.4",
    "react-toastify": "^7.0.2",
    "slugify": "^1.5.0",
    "socket.io": "^4.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
